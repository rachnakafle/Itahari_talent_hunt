<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SchoolRegistrationForm</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SchoolRegistrationForm.styles.css" asp-append-version="true" />
</head>
<body>
    <div class="container">
        <div class="card content-card">
            <div class="content-row row">
                <div class="col-lg-4 content-image">
                    <img src="~/images/health.jpg" />
                </div>
                <div class="col-lg-8 content-entry">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <partial name="_ValidationScriptsPartial" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>